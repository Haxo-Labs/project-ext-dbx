{
  "name": "dbx",
  "version": "0.1.6",
  "description": "NAPI bindings for DBX Universal Database SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "dbx",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "i686-pc-windows-msvc",
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-freebsd"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "universal": "napi universal",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "napi-rs",
    "NAPI",
    "napi",
    "node-api",
    "dbx",
    "database",
    "universal",
    "api",
    "bindings"
  ],
  "author": "Effortless Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/effortlesslabs/dbx.git"
  },
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.0",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^1.0.0",
    "typescript": "^5.8.3",
    "vitest": "^1.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dbx/*.node"
  ],
  "optionalDependencies": {
    "dbx-win32-x64-msvc": "0.1.6",
    "dbx-darwin-x64": "0.1.6",
    "dbx-linux-x64-gnu": "0.1.6",
    "dbx-win32-ia32-msvc": "0.1.6",
    "dbx-darwin-arm64": "0.1.6",
    "dbx-linux-x64-musl": "0.1.6",
    "dbx-linux-arm64-gnu": "0.1.6",
    "dbx-linux-arm-gnueabihf": "0.1.6",
    "dbx-linux-arm64-musl": "0.1.6",
    "dbx-freebsd-x64": "0.1.6"
  }
}
